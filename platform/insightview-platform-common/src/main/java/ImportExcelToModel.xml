		<property name="assetName" column="1" excelTitleName="资产名称" isNull="N" columnWidth="12" maxLength="20" dataType="String" fixity="NO" codeTableName="" default="" greaterThan="" />
		<property name="resAssetType" column="6" excelTitleName="资产类型" isNull="N"  columnWidth="12" maxLength="20" dataType="String" fixity="NO" codeTableName="" default="" greaterThan="" />
		<property name="status" column="4" excelTitleName="资产状态" isNull="Y"  columnWidth="60" maxLength="5" dataType="String" fixity="NO" codeTableName="" default="" greaterThan="" />
		<property name="receiveMode" column="6" excelTitleName="资产来源" isNull="Y"  columnWidth="12" maxLength="100" dataType="String" fixity="NO" codeTableName="" default="" greaterThan="" />
		<property name="address" column="6" excelTitleName="位置" isNull="Y"  columnWidth="12" maxLength="50" dataType="String" fixity="NO" codeTableName="" default="" greaterThan="" />
		<property name="description" column="7" excelTitleName="描述" isNull="Y"  columnWidth="12" maxLength="200" dataType="String" fixity="NO" codeTableName="" default="" greaterThan="" />
