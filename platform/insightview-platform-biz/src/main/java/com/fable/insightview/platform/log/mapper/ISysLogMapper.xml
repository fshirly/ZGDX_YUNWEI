<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper
	namespace="com.fable.insightview.platform.log.mapper.ISysLogMapper">
	
	<select id="insertSysLog" parameterType="SysLog">
	  insert into SysLog(id,actor,module,content,option_time,result,operation) 
	  values(#{id},#{actor},#{module},#{content},#{optionTime},#{result},#{operation})
	</select>
</mapper>

